<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
    PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-config.dtd">
 
<configuration>
       
    <typeAliases>
        <typeAlias type="com.project.user.User" alias="User" />
    	<typeAlias type="com.project.review.Review" alias="Review"/>
    	<typeAlias type="com.project.refri.Refri" alias="Refri"/>
    	<typeAlias type="com.project.ingre.Ingredients" alias="Ingre"/>
    	<typeAlias type="com.project.alarm.Alarm" alias="Alarm"/>
    	<typeAlias type="com.project.comment.Comment" alias="Comment"/>
