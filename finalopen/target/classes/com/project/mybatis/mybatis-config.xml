    	<typeAlias type="com.project.memo.Memo" alias="Memo"/>
    </typeAliases>
 
    <mappers>
        <mapper resource="/com/project/mybatis/UserMapper.xml" />
        <mapper resource="/com/project/mybatis/RefriMapper.xml" />
    	<mapper resource="/com/project/mybatis/ReviewMapper.xml"/>
    	<mapper resource="/com/project/mybatis/IngreMapper.xml"/>
    	<mapper resource="/com/project/mybatis/AlarmMapper.xml"/>
    	<mapper resource="/com/project/mybatis/CommentMapper.xml"/>
